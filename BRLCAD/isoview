#!/bin/sh
env /usr/brlcad/dev-7.25.0/bin//rt -M \
 -o isoview.pix\
 $*\
 '/home/amit/public_html/cgi-bin/BRLCAD/ballbearing.g'\
 'ballbearing.c' \
 2>> isoview.log\
 <<EOF
viewsize 8.80000000000000e+02;
orientation 2.50000000000000e-01 4.33012701892219e-01 7.50000000000000e-01 4.33012701892219e-01;
eye_pt 1.17315353299546e+03 6.60000000000000e+02 7.62102355330306e+02;
start 0; clean;
end;

EOF
