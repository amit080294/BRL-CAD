#!/bin/sh
env /usr/brlcad/dev-7.25.0/bin//rt -M \
 -o topview.pix\
 $*\
 '/home/amit/public_html/cgi-bin/BRLCAD/ballbearing.g'\
 'ballbearing.c' \
 2>> topview.log\
 <<EOF
viewsize 8.80000000000000e+02;
orientation 1.22464679914735e-16 1.49975978266186e-32 1.22464679914735e-16 1.00000000000000e+00;
eye_pt 3.00000000000000e+01 0.00000000000000e+00 1.52420471066061e+03;
start 0; clean;
end;

EOF
